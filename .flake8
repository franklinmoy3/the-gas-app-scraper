[flake8]
exclude =
    .devcontainer,
    .git,
    .github,
    __pycache__,
    docs,
    build,
    dist
max-complexity = 10
max-line-length = 88 
plugins = flake8_annotations
select = B,E
extend-ignore = 
    ; E501: line too long (Delegated to black)
    E501,
    ; E203: whitespace before ':' (Contradicts black)
    E203
